{% extends 'base.html.twig' %}

{% block body %}
    <h1>Detallecompra</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ detalleCompra.id }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ detalleCompra.cantidad }}</td>
            </tr>
            <tr>
                <th>Subtotal</th>
                <td>{{ detalleCompra.subtotal }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('detallecompra_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('detallecompra_edit', { 'id': detalleCompra.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
